<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Iways_HomePage::css/iways_homepage.css" />
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Iways\HomePage\Block\Homepage"
                   name="iways_homepage_block_homepage"
                   as="iways_homepage_block_homepage"
                   template="Iways_HomePage::homepage.phtml"
                   before="-" />
            <block class="Iways\HomePage\Block\Topseller"
                   name="iways_homepage_block_topseller"
                   as="iways_homepage_block_topseller"
                   template="Iways_HomePage::topseller.phtml"
                   after="iways_homepage_block_homepage" />
        </referenceContainer>
    </body>
</page>

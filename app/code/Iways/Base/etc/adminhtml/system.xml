<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="iways" translate="label" sortOrder="000" class="iways">
            <label>i-ways</label>
        </tab>
        <section id="iways_base" translate="label" type="text" sortOrder="000"
                 showInDefault="1" showInWebsite="1" showInStore="1">
            <label>General</label>
            <tab>iways</tab>
            <resource>Iways_Base::config</resource>
            <group id="iways_base_information" translate="label" type="text" sortOrder="000"
                   showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Information</label>
                
                <field id="iways_base_information_check" translate="label comment" sortOrder="000" 
                       type="hidden" showInDefault="1" showInWebsite="1" showInStore="1">
                    <frontend_model>Iways\Base\Block\Adminhtml\System\Config\Form\Field\Check</frontend_model>
                </field>
                
                <field id="iways_base_information_notification" translate="label comment" sortOrder="111" 
                       type="note" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Warning</label>
                    <depends>
                        <field id="iways_base_information_check">1</field>
                    </depends>
                    <comment>Please install an i-ways Certified Frontend Theme in order to assure full compatibility with our extensions!</comment>
                </field>

                <field id="iways_base_information_credits" type="note" sortOrder="999"
                       showInDefault="1" showInWebsite="1" showInStore="1">
                    <label><![CDATA[Copyright &copy; 2011-2017<br /><br /><a href="http://www.i-ways.net" target="_blank">i-ways sales solutions GmbH<br />Kurf√ºrstendamm 125A<br />D-10711 Berlin</a><br /><br />]]></label>
                    <comment><![CDATA[<br /><img src="data:image/gif;base64," alt="i-ways sales solutions GmbH" />]]></comment>
                </field>
            </group>
        </section>
    </system>
</config>

<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="vollkorn" translate="label" sortOrder="000" class="vollkorn">
            <label>Vollkorn</label>
        </tab>
        <section id="vollkorn_all" translate="label" type="text" sortOrder="000"
                 showInDefault="1" showInWebsite="1" showInStore="1">
            <label>General</label>
            <tab>vollkorn</tab>
            <resource>Vollkorn_All::config</resource>
            <group id="vollkorn_all_information" translate="label" type="text" sortOrder="000"
                   showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Information</label>
                <field id="vollkorn_all_information_credits" type="note" sortOrder="000"
                       showInDefault="1" showInWebsite="1" showInStore="1">
                    <label><![CDATA[<br />Copyright &copy; 2011-2017 Lord Vollkorn<br /><br />]]></label>
                    <comment><![CDATA[<img src="data:image/png;base64," alt="i-ways sales solutions GmbH" />]]></comment>
                </field>
            </group>
        </section>
    </system>
</config>
